  },
  "devDependencies": {
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@types/react": "17.0.16",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.0.6",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "jest": "^27.0.6",
    "jest-css-modules": "^2.1.0",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.7",
    "typescript": "4.3.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "jest": {
